{
	"manifest_version": 2,
	"name": "The Scarlet Letter",
	"version": "1.0",
	"description": "find fake reveiws",
	"permissions": [
        "storage",
        "activeTab"
    ],
	"background": {
		"scripts": ["background.js"],
		"persistent": false
	  },
	  "content_scripts": [
		{
		  "matches": ["<all_urls>"],
		  "js": ["content.js", "save.js"]
		}
	  ],
	  "browser_action": {
		"default_popup": "popup.html"
	  }
  }